   "source": [
    "def is_plaindorme_string(st):\n",
    "    l_pos = 0\n",
    "    r_pos = len(st)-1\n",
    "    while r_pos >= l_pos:\n",
    "        if st[l_pos] != st[r_pos]:\n",
    "            return False\n",
    "        l_pos += 1\n",
    "        r_pos -= 1\n",
    "    return True"
   ]
