    "def is_plaindorme(x):\n",
    "    if x < 0:\n",
    "        return False\n",
    "    number = x\n",
    "    reverse = 0\n",
    "    while number:\n",
    "        reverse = reverse*10 + number%10\n",
    "        number //= 10\n",
    "    return x == reverse\n",
    "        "
   ]
