   "source": [
    "def find_pairs(nums , t_value):\n",
    "    nums_set = set(nums)\n",
    "    pairs = []\n",
    "    for x in nums_set :\n",
    "        c = t_value -x\n",
    "        if c in nums_set:\n",
    "            pairs.append({x,c})\n",
    "    return pairs        "
   ]
