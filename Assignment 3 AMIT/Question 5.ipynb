   "source": [
    "def is_prime(x):\n",
    "    if x==1:\n",
    "        return False\n",
    "    elif x==2:\n",
    "        return True\n",
    "    else:\n",
    "        for num in range(2,x):\n",
    "            if x % num == 0:\n",
    "                return False\n",
    "        return True"
   ]
